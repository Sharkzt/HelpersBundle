{
  "name" : "sharkzt/helpersbundle",
  "description" : "Helpers bundle, that extends symfony validator, html purifier classes for easy usage",
  "keywords": ["framework", "helper", "validation", "purification"],
  "version" : "0.2.0",
  "type" : "symfony-bundle",
  "homepage": "https://github.com/Sharkzt/HelpersBundle",
  "license" : "MIT",
  "authors" : [{
    "name" : "Alexander Pysarchuk",
    "email" : "then.bemyguest@gmail.com"
  }],
  "require" : {
    "php": ">=7.0.0",
    "symfony/symfony": ">=3.3",
    "ezyang/htmlpurifier": ">=4.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "sensio/generator-bundle": "~3.0",
    "symfony/phpunit-bridge": "3.3.*@dev"
  },
  "autoload" : {
    "psr-4" : { "Sharkzt\\HelpersBundle\\" : "" }
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "0.2.x-dev"
    }
  }
}